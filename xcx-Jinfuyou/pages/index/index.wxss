/* 商品列表 新 start */
.shop_min{
  margin-top: 20rpx;
  padding: 0 20rpx;
  width: 100%;
  background: #fff;
  box-sizing: border-box;
}
.recommend{
  width: 100%;
  height: 124rpx;
  text-align: center;
  line-height: 124rpx;
}
.shop_lists{
  width: 100%;
  background: #fff;
  display: flex;
  flex-direction:row;
 flex-wrap:wrap;
 justify-content:space-between;
 
}
