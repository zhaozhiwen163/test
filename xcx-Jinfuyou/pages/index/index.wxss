/*没有授权*/

.noPosition {
  padding: 30rpx;
  padding-top: 600rpx;
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  top: 0;
  margin: auto;
  background: url(data:image/png;base64,) no-repeat center 200rpx;
  background-size: 245rpx 245rpx;
  text-align: center;
  overflow: hidden;
}

.noPosition text {
  display: inline-block;
  line-height: 80rpx;
  overflow: hidden;
  font-size: 32rpx;
  margin-right: 20rpx;
  color: #333;
}

.noPosition .button {
  display: inline-block;
  color: #fff;
  overflow: hidden;
  font-size: 32rpx;
  background: linear-gradient(#38ae3b, #226d25);
  box-shadow: 2rpx 2rpx 6rpx #154216;
  width: 170rpx;
  height: 90rpx;
  line-height: 90rpx;
  border-radius: 10rpx 10rpx;
}

/*加载中*/

.load {
  width: 100%;
  height: 150rpx;
  background-color: #f5f5f5;
  line-height: 150rpx;
  text-align: center;
  position: relative;
}

.load .change {
  width: 100rpx;
  height: 20rpx;
  position: absolute;
  top: 70rpx;
  right: 200rpx;
}

.load .change view {
  width: 10rpx;
  height: 10rpx;
  border-radius: 50% 50%;
  background-color: #fff;
  border: 1rpx solid #333;
  float: left;
  margin-left: 15rpx;
}

.load .loadstyle {
  width: 275rpx;
  height: auto;
  margin: 0 auto;
}

.load .change .on {
  background: #333;
}

.load .loadover {
  color: #333;
}

.load .loadover view {
  width: 150rpx;
  height: 1rpx;
  background: #333;
}

.load .loadover .left {
  position: absolute;
  top: 75rpx;
  left: 125rpx;
}

.load .loadover .right {
  position: absolute;
  top: 75rpx;
  right: 125rpx;
}
/* --tab-swich-start */
.tab-swich {
  background-color: #fff;
  display: flex;
  font-size: 28rpx;
  color: #666;
  text-align: center;
  margin-top: 20rpx;
}
.tab-swich text {
  flex: 1;
  line-height: 100rpx;
  border-bottom: 1rpx solid #999;
  transition: all .5s;
}
.tab-swich .on {
  border-bottom-color: #f60;
  font-size: 32rpx;
  color: #f60;
}

/* picker  Start */
.crop-wrap {
  margin: 20rpx 30rpx;
  border: 1rpx solid #ddd;
  border-radius: 6rpx;
  line-height: 75rpx;
  text-align: center;
  height: 75rpx;
  background-color: #fff;
  margin-bottom: 20rpx;
}
/* picker  END */