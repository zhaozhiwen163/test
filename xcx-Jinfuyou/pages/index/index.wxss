}
.banner image {
  width: 100%;
  height: 100%;
}

/* banner图dots*/

.banner .wx-swiper-dots.wx-swiper-dots-horizontal {
  margin-bottom: 20rpx;
}

.banner .wx-swiper-dot {
  display: inline-flex;
  width: 20rpx;
  height: 20rpx;
  margin-left: 40rpx;
  border-radius:50% 50%;
  z-index: 50;
  background: #fff;
  border: 1px solid #2a415c;
}

.banner .wx-swiper-dot-active {
  background: #2a415c;
}

.shopnone {
  width: 100%;
  height: 150rpx;
  line-height: 150rpx; 
  text-align: center;
  color: #999;
  font-size: 38rpx;
}

