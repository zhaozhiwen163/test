/* pages/orderDetail/orderDetail.wxss */
/* 轮播 */
.adf {
  padding-bottom: 100rpx;
}
.banner {
  width: 100%;
  height: 750rpx;
  position: relative;
  background: #fff;
  margin: 0 auto
}
.banner swiper-item {
  text-align: center
}
.banner image {
    width: 100%;  
    height: 100%;  
}
.mask{
  position:fixed;
  top:0rpx;
  left:0rpx;
  z-index:200;
}
.errorMsg {
  background-color: #f60;
  text-align: center;
  padding: 0 30rpx;
  font-size: 30rpx;
  line-height: 50rpx;
  height: 50rpx;
  color: #fff;
  position: fixed;
  bottom: 100rpx;
  left: 65rpx;
  z-index: 200;
}

.touming {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0rpx;
  left: 0rpx;
}

/* banner图dots*/

.container .swipe-btn-wrapper {
  width: 100%;
  height: 15rpx;
  position: absolute;
  left: 0;
  right: 0;
  top: 710rpx;
  display: flex;
  z-index: 10;
}

.container .swipe-btn-wrapper .swipe-btn-list {
  width: auto;
  height: 15rpx;
  margin-left: 34.33%;
  text-align: center;
}

.container .swipe-btn-wrapper .swipe-btn-list view {
  border-radius: 50% 50%;
  background: white;
  float: left;
  margin: 0rpx 20rpx;
}

.container .swipe-btn-wrapper .swipe-btn-list .on {
  background: #2a415c;
}

.banner .wx-swiper-dots.wx-swiper-dots-horizontal {
  margin-bottom: 20rpx;
}

.banner .wx-swiper-dot {
  display: inline-flex;
  width: auto;
  height: 15rpx;
  margin-left: 40rpx;
  z-index: 50;
}

.banner .wx-swiper-dot::before {
  content: '';
  padding: 9rpx 9rpx;
  flex-grow: 1;
  background: #999;
  border-radius: 50% 50%;
}

.banner .wx-swiper-dot-active::before {
  background: #333;
}

.shop-detail {
  width: 555rpx;
  position: relative;
  margin-top: 1px;
  margin-left: 25rpx;
}

.bargin {
  width: 130rpx;
  min-height: 130rpx;
  float: right;
  position: absolute;
  top: 20rpx;
  right: 20rpx;
  z-index: 150;
  background: #fff url('data:image/png;base64,') no-repeat center center;
  background-size: 100% 100%;
}

.shopDetail {
  width: 100%;
  position: relative;
  background-color: #fff;
}

.shop-detail .shop-name {
  width: 100%;
  font-size: 35rpx;
  padding-top: 20rpx;
  margin-bottom: 20rpx;
  color: #333;
   overflow: hidden; 
}

.bought {
  width: 230rpx;
  height: 50rpx;
  line-height: 100rpx;
  text-align: center;
  float: left;
  font-size: 30rpx;
}

.shop-detail .company-name {
  width: 100%;
  font-size: 30rpx;
  color: #d9d9d9;
  line-height: 40rpx;
  overflow: hidden;
  /* margin-top: 10rpx; */
}

.shop-detail .shop-price {
  font-size: 30rpx;
  color: #e4383b;
  padding-bottom: 20rpx;
  padding-top: 10rpx;
}

.shop-detail .shop-price text {
  font-size: 42rpx;
  font-weight: 600;
}

/*规格*/
.eqwith {
  width: 175rpx;
  text-align: right;
}
.spec {
  width: 100%;
  height: auto;
  background: #fff;
  margin-top: 20rpx;
  line-height: 100rpx;
  display: flex;
}

.spec .sp {
  margin-left: 38rpx;
  color: #b0b0b0;
  font-size: 30rpx;
  display: inline-block;
  /* width: 175rpx; */
  height: auto;
  text-align: right;
}

.spec .rule {
  float: left;
  width: 650rpx;
  height: auto;
}

.spec .rule text {
  margin-left: 65rpx;
  font-size: 30rpx;
  text-align: center;
  line-height: 2;
  color: #333;
  display: inline-block;
  border: 1px solid #ddd;
  padding: 0rpx 20rpx;
}

.spec .rule .choose {
  border: 1px solid #390;
  color: #390;
}

/*数量*/

.shop-number {
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  margin-top: 20rpx;
  background: #fff;
  color: #b0b0b0;
}

.shop-number .number {
  /* width: 150rpx; */
  height: auto;
   float: left; 
   margin-left: 14rpx; 
  font-size: 30rpx;
  text-align: right;
}

.shop-number .operation {
  color: #333;
  float: left;
  margin-left: 74rpx;
  margin-top: 22rpx;
}

operation-result {
  color: #333;
}

/*结算*/

.balance {
  width: 100%;
  height: 100rpx;
  text-align: center;
  margin-top: 20rpx;
  position: fixed;
  bottom: 0px;
  left: 0px;
  z-index: 100;
}

.balance .shopping-car {
  width: 50%;
  height: 100rpx;
  float: left;
  background: #390;
  font-size: 35rpx;
  color: white;
  line-height: 100rpx;
}

.balance .purchase {
  width: 50%;
  height: 100rpx;
  float: left;
  background: #e4383b;
  font-size: 35rpx;
  color: white;
  line-height: 100rpx;
}

