}

.pddeion .ption, .add {
  flex: 2;
  margin-left: 25rpx;
  padding: 20rpx;
  padding-left: 0;
  display: flex;
}

.pddeion .ption .region-txt, .recive {
  color: #949494;
  font-size: 30rpx;
  display: inline-block;
  /* width: 125rpx; */
  height: auto;
  text-align: right;
}

.pddeion .ption .indition-txt, .addt {
  color: #333;
